(set-logic QF_UFDTLIA)
(declare-fun q (Int) Int)
(define-fun f ((a Int)) Int (+ a 1))

(define-system gconsttst
  :input ((a Int))
  :output ((b Int))
  ; :inv (forall ((q Int)) (= 1 2))
  :init (= b 1)
  :trans (ite (= a 1) (= b' (+ b 1)) (= b' b))
)

(check-system gconsttst
  :input ((a Int))
  :output ((b Int))
  :reachable (rprop1 (= b 0))
  :reachable (rprop2 (= a 1000))
  :query (q1 (rprop1)) 
  :query (q2 (rprop2)) 
  :query (q3 (rprop1 rprop2)) 
)
